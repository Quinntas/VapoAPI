{
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "regions": [
    "gru1"
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "main.py"
    },
    {
      "source": "/(.*)",
      "destination": "src/python/api/refresh.py"
    },
    {
      "source": "/api/v1/store/(.*)",
      "destination": "src/python/routers/v1/store.py"
    }
  ]
}